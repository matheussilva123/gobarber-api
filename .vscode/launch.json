  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug",
      "protocol": "inspector",
      "restart": true,
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
